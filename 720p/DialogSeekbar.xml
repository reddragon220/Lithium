<window id="115">
<defaultcontrol>2</defaultcontrol>
<visible>
	player.seeking | player.displayafterSeek | player.paused | player.forwarding | player.rewinding
</visible>

<controls>
		<control type="group">
			<visible>!Window.IsVisible(videoosd) + Player.HasVideo</visible>
			<control type="image">
				<posx>208</posx>
				<posy>617</posy>
		
				<width>862</width>
				<height>50</height>
		
				<texture>Zen/panes/osd/osd_small_bg.png</texture>
				<aspectratio>keep</aspectratio>
			</control>

			<control type="progress" id="10">
				<onup>10</onup>
				<ondown>901</ondown>
		
				<posx>233</posx>
				<posy>627</posy>
		
				<width>572</width>
				<height>30</height>
		
				<visible>true</visible>
				<reveal>true</reveal>
				<texturebg>Zen/elements/osd/progress_bg.png</texturebg>
				<midtexture>Zen/elements/osd/progress_mid.png</midtexture>
				<lefttexture></lefttexture>
				<righttexture>-</righttexture>
				<overlaytexture>-</overlaytexture>
				<info>Player.Progress</info>
			</control>

			<control type="label">
				<width>300</width>
				<posx>783</posx>
				<posy>627</posy>

				<font>Small</font>
				<textcolor>ffe8e8db</textcolor>
				<label>$INFO[Player.Time]/$INFO[Player.Duration ]</label>
				<align>center</align>
			</control>

			<control type="image">
				<posx>208</posx>
				<posy>617</posy>
		
				<width>862</width>
				<height>50</height>
		
				<texture>Zen/panes/osd/osd_small_shadow.png</texture>
				<aspectratio>keep</aspectratio>
			</control>

			<control type="image">
				<posx>208</posx>
				<posy>617</posy>
		
				<width>862</width>
				<height>50</height>
		
				<texture>Zen/panes/osd/osd_small_outline.png</texture>
				<aspectratio>keep</aspectratio>
			</control>

			<control type="group">
				<visible>!Player.Playing</visible>
				<control type="image">
					<posx>529</posx>
					<posy>667</posy>

					<width>222</width>
					<height>32</height>

					<texture>Zen/panes/osd/pop_up_bg.png</texture>
				</control>

				<control type="label">
					<visible>Player.Paused</visible>
					<posy>665</posy>
					<posx>529</posx>
					<width>222</width>
					<align>center</align>
					<label>Paused</label>
					<font>Small</font>
					<textcolor>ffe8e8db</textcolor>
				</control>

				<control type="label">
					<visible>Player.Seeking</visible>
					<posy>665</posy>
					<posx>529</posx>
					<width>222</width>
					<align>center</align>
					<label>Seeking</label>
					<font>Small</font>
					<textcolor>ffe8e8db</textcolor>
				</control>

				<control type="label">
					<visible>Player.Forwarding2x</visible>
					<posy>665</posy>
					<posx>529</posx>
					<width>222</width>
					<align>center</align>
					<label>Fast Foward 2x</label>
					<font>Small</font>
					<textcolor>ffe8e8db</textcolor>
				</control>
				<control type="label">
					<visible>Player.Forwarding4x</visible>
					<posy>665</posy>
					<posx>529</posx>
					<width>222</width>
					<align>center</align>
					<label>Fast Foward 4x</label>
					<font>Small</font>
					<textcolor>ffe8e8db</textcolor>
				</control>
				<control type="label">
					<visible>Player.Forwarding8x</visible>
					<posy>665</posy>
					<posx>529</posx>
					<width>222</width>
					<align>center</align>
					<label>Fast Foward 8x</label>
					<font>Small</font>
					<textcolor>ffe8e8db</textcolor>
				</control>
				<control type="label">
					<visible>Player.Forwarding16x</visible>
					<posy>665</posy>
					<posx>529</posx>
					<width>222</width>
					<align>center</align>
					<label>Fast Foward 16x</label>
					<font>Small</font>
					<textcolor>ffe8e8db</textcolor>
				</control>
				<control type="label">
					<visible>Player.Forwarding32x</visible>
					<posy>665</posy>
					<posx>529</posx>
					<width>222</width>
					<align>center</align>
					<label>Fast Foward 32x</label>
					<font>Small</font>
					<textcolor>ffe8e8db</textcolor>
				</control>
				<control type="label">
					<visible>Player.Rewinding2x</visible>
					<posy>665</posy>
					<posx>529</posx>
					<width>222</width>
					<align>center</align>
					<label>Rewind 2x</label>
					<font>Small</font>
					<textcolor>ffe8e8db</textcolor>
				</control>
				<control type="label">
					<visible>Player.Rewinding4x</visible>
					<posy>665</posy>
					<posx>529</posx>
					<width>222</width>
					<align>center</align>
					<label>Rewind 4x</label>
					<font>Small</font>
					<textcolor>ffe8e8db</textcolor>
				</control>
				<control type="label">
					<visible>Player.Rewinding8x</visible>
					<posy>665</posy>
					<posx>529</posx>
					<width>222</width>
					<align>center</align>
					<label>Rewind 8x</label>
					<font>Small</font>
					<textcolor>ffe8e8db</textcolor>
				</control>
				<control type="label">
					<visible>Player.Rewinding16x</visible>
					<posy>665</posy>
					<posx>529</posx>
					<width>222</width>
					<align>center</align>
					<label>Rewind 16x</label>
					<font>Small</font>
					<textcolor>ffe8e8db</textcolor>
				</control>
				<control type="label">
					<visible>Player.Rewinding32x</visible>
					<posy>665</posy>
					<posx>529</posx>
					<width>222</width>
					<align>center</align>
					<label>Rewind 32x</label>
					<font>Small</font>
					<textcolor>ffe8e8db</textcolor>
				</control>

				<control type="image">
					<posx>529</posx>
					<posy>667</posy>

					<width>222</width>
					<height>32</height>

					<texture>Zen/panes/osd/pop_up_shadow.png</texture>
				</control>
				<control type="image">
					<posx>529</posx>
					<posy>667</posy>

					<width>222</width>
					<height>32</height>

					<texture>Zen/panes/osd/pop_up_outline.png</texture>
				</control>

			</control>
			<control type="group">
				<visible>Player.Caching + !Player.Playing</visible>
				<control type="image">
					<posx>529</posx>
					<posy>667</posy>

					<width>222</width>
					<height>32</height>

					<texture>Zen/panes/osd/pop_up_bg.png</texture>
				</control>

				<control type="label">
					<visible>Player.Caching</visible>
					<posy>665</posy>
					<posx>529</posx>
					<width>222</width>
					<align>center</align>
					<label>Caching...</label>
					<font>Small</font>
					<textcolor>ffe8e8db</textcolor>
				</control>

				<control type="image">
					<posx>534</posx>
					<posy>667</posy>

					<width>22</width>
					<height>32</height>

					<texture>Zen/panes/osd/pop_up_shadow.png</texture>
				</control>
				<control type="image">
					<posx>529</posx>
					<posy>667</posy>

					<width>22</width>
					<height>32</height>

					<texture>Zen/panes/osd/pop_up_outline.png</texture>
				</control>

			</control>
		
		


		
			
		</control>
</controls>

