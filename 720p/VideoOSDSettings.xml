<window id="123">
	<defaultcontrol always="true">5</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<animation type="WindowOpen" reversible="false">
		<effect type="zoom" start="80" end="100" center="640,360" delay="200" easing="out" tween="back" time="300" />
		<effect type="fade" delay="200" start="0" end="100" time="300" />
	</animation>
	<animation type="WindowClose" reversible="false">
		<effect type="zoom" start="100" end="80" center="640,360" easing="in" tween="back" time="300" />
		<effect type="fade" start="100" end="0" time="300" />
	</animation>
	
	<controls>
		<control type="group">
			<posx>221</posx>
			<posy>66</posy>
			<control type="image">
				<width>848</width>
				<height>506</height>
				<texture>osd/settings/dialog_bg.png</texture>
			</control>
			<control type="grouplist" id="5">
				<description>Control Area</description>
				<posx>5</posx>
				<posy>18</posy>
				<width>838</width>
				<height>488</height>
				<itemgap>0</itemgap>
				<orientation>vertical</orientation>
				<pagecontrol>60</pagecontrol>
				<onleft>28</onleft>
				<onright>28</onright>
				<onup>28</onup>
				<ondown>28</ondown>
			</control>
			<control type="button" id="7">
				<width>838</width>
				<height>47</height>
			
				<textcolor>7fffffff</textcolor>
				<focusedcolor>ffe8e8db</focusedcolor>
				<disabledcolor>7f7c7c72</disabledcolor>
				<shadowcolor>7f000000</shadowcolor>
				<font>OSDSettings</font>
		    	<textoffsetx>20</textoffsetx>
			
				<texturefocus>osd/settings/dialog_highlight_focused.png</texturefocus>
