<window id="12">
	<defaultcontrol always="true">3</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>
		<control type="image">
			<width>1280</width>
			<height>720</height>
			<texture>settings/settings_bg.png</texture>
			<aspectratio>stretch</aspectratio>
		</control>
		
		<control type="image">
			<posx>293</posx>
			<width>987</width>
			<height>720</height>
			<texture>settings/bar_bg_long.png</texture>
			<aspectratio>stretch</aspectratio>
		</control>

		<control type="grouplist" id="5">
			<description>Control Area</description>
			<posx>293</posx>
			<width>962</width>
			<height>720</height>
			<onleft>3</onleft>
			<onright>3</onright>
			<onup>5</onup>
			<ondown>5</ondown>
			<orientation>vertical</orientation>
		</control>

		<control type="image">
			<width>297</width>
			<height>720</height>
			<texture>settings/bar_bg.png</texture>
			<aspectratio>stretch</aspectratio>
		</control>
				
		<control type="grouplist" id="3">
			<description>Button Area</description>
			<posx>0</posx>
			<posy>0</posy>
			
			<width>294</width>
			<height>720</height>
			
			<onleft>5</onleft>
			<onright>5</onright>
			<onup>3</onup>
			<ondown>3</ondown>
			<orientation>vertical</orientation>
		</control>


		<control type="button" id="10">
			<description>Default Category Button</description>
			<height>50</height>
			<width>294</width>
	    	<textcolor>7fffffff</textcolor>
			<focusedcolor>ffe8e8db</focusedcolor>
			<disabledcolor>7f7c7c72</disabledcolor>
			<shadowcolor>7f000000</shadowcolor>
			<font>Title</font>
	    	<textoffsetx>20</textoffsetx>

	    	<texturefocus>settings/highlight_category.png</texturefocus>
			<texturenofocus>settings/category_divider.png</texturenofocus>
		</control>


		<control type="button" id="7">
			<description>Default Button</description>
			<height>50</height>
			<width>962</width>
	    	<textcolor>7fffffff</textcolor>
			<focusedcolor>ffe8e8db</focusedcolor>
			<disabledcolor>7f7c7c72</disabledcolor>
			<shadowcolor>7f000000</shadowcolor>
			<font>Title</font>
	    	<textoffsetx>20</textoffsetx>

	    	<texturefocus>settings/highlight_long.png</texturefocus>
	    	<texturenofocus>settings/large_divider.png</texturenofocus>
		</control>
		<control type="radiobutton" id="8">
			<description>Default Radio Button</description>
			<height>50</height>
			<width>962</width>
	    	<textcolor>7fffffff</textcolor>
			<focusedcolor>ffe8e8db</focusedcolor>
			<disabledcolor>7f7c7c72</disabledcolor>
			<shadowcolor>7f000000</shadowcolor>
			<font>Title</font>
	    	<textoffsetx>20</textoffsetx>
			<align>left</align>
	
			<texturefocus>settings/highlight_long.png</texturefocus>
	    	<texturenofocus>settings/large_divider.png</texturenofocus>

			<radioposx>877</radioposx>
			<radiowidth>65</radiowidth>
			<radioheight>37</radioheight>
			<textureradiofocus>settings/on.png</textureradiofocus>
			<textureradionofocus>settings/off.png</textureradionofocus>
		</control>
		<control type="spincontrolex" id="9">
			<description>Default Spin Control</description>
			<height>50</height>
			<width>962</width>
	    	<textcolor>7fffffff</textcolor>
			<focusedcolor>ffe8e8db</focusedcolor>
			<disabledcolor>7f7c7c72</disabledcolor>
			<shadowcolor>7f000000</shadowcolor>
			<font>Title</font>
	    	<textoffsetx>20</textoffsetx>

			<texturefocus>settings/highlight_long.png</texturefocus>
	    	<texturenofocus>settings/large_divider.png</texturenofocus>


			<spinposx>0</spinposx>
			<spinwidth>65</spinwidth>
			<spinheight>37</spinheight>
			<textureup>settings/spin_up_unfocus.png</textureup>
			<textureupfocus>settings/spin_up_focus.png</textureupfocus>
			<texturedown>settings/spin_down_unfocus.png</texturedown>
			<texturedownfocus>settings/spin_down_focus.png</texturedownfocus>

			
		</control>
	</controls>
</window>
