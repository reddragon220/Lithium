<window id="2000">
<defaultcontrol always="true">3</defaultcontrol>
<allowoverlay>yes</allowoverlay>
  
<coordinates>
  <system>1</system>
  <posx>100</posx>
  <posy>80</posy>
</coordinates>
  
<controls>
    
<include>DialogBack</include>

  <control type="label">
	  <include>DialogID</include>
	  <label>DialogSelect.xml</label>
  </control>

  <control type="list" id="3">
	  <posx>10</posx>
	  <posy>40</posy>
    <width>340</width>
	  <height>382</height>
	  <onleft>5</onleft>
	  <onright>5</onright>
	  <onup>3</onup>
	  <ondown>3</ondown>
	  <itemlayout height="32">
      <control type="image">
        <width>340</width>
        <height>30</height>
        <texture>ButtonNF.png</texture>
      </control>
		  <control type="label">
			  <posx>5</posx>
			  <width>330</width>
        <height>30</height>
			  <info>ListItem.Label</info>
        <scroll>false</scroll>
		  </control>
	  </itemlayout>
	  <focusedlayout height="32">
		  <control type="image">
			  <width>340</width>
			  <height>30</height>
			  <texture>ButtonFo.png</texture>
		  </control>
		  <control type="label">
			  <posx>5</posx>
			  <width>330</width>
			  <height>30</height>
        <textcolor>White</textcolor>
			  <info>ListItem.Label</info>
		  </control>
	  </focusedlayout>
  </control>
  
  <control type="label" id="2">
    <description>Number Of Files</description>
    <posx>480</posx>
    <posy>10</posy>
    <width>100</width>
    <label>-</label>
    <align>right</align>
  </control>
  
  <control type="button" id="5">
    <description>Manual</description>
    <posx>360</posx>
    <posy>40</posy>
    <label>$LOCALIZE[368]</label>
    <texturefocus border="1">ButtonFo.png</texturefocus>
    <texturenofocus border="1">ButtonNF.png</texturenofocus>
    <onleft>3</onleft>
    <onright>3</onright>
    <onup>3</onup>
    <ondown>3</ondown>
  </control>

</controls>
    
</window>