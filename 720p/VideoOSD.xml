<window id="2901">
<defaultcontrol>4</defaultcontrol>
	
<controls>
		
	
		<control type="image">
			<posx>0</posx>
			<posy>637</posy>
		
			<width>1280</width>
			<height>83</height>
		
			<texture>osd/osd_bg.png</texture>
			<aspectratio>-</aspectratio>
		</control>
		
		<control type="grouplist" id="101">
			<posx>465</posx>
			<posy>666</posy>
			<width>360</width>
			<height>119</height>
			<orientation>horizontal</orientation>
			<scrolltime>200</scrolltime>
			<itemgap>9</itemgap>
		

			<control type="button" id="1">
				<width>51</width>
				<height>51</height>
		
				<onleft>1</onleft>
				<onright>2</onright>

				<onclick>xbmc.playercontrol(previous)</onclick>

				<texturefocus>osd/buttons/previous_focused.png</texturefocus>
				<texturenofocus>osd/buttons/previous_unfocused.png</texturenofocus>
			</control>
			<control type="button" id="2">
				<width>51</width>
				<height>51</height>
		
				<onleft>1</onleft>
				<onright>3</onright>

				<onclick>xbmc.playercontrol(rewind)</onclick>

				<texturefocus>osd/buttons/rewind_focused.png</texturefocus>
				<texturenofocus>osd/buttons/rewind_unfocused.png</texturenofocus>
			</control>
			<control type="button" id="3">
				<width>51</width>
				<height>51</height>
		
				<onleft>2</onleft>
				<onright>4</onright>

				<onclick>xbmc.playercontrol(stop)</onclick>

				<texturefocus>osd/buttons/stop_focused.png</texturefocus>
				<texturenofocus>osd/buttons/stop_unfocused.png</texturenofocus>
			</control>
			<control type="togglebutton" id="4">
				<width>51</width>
				<height>51</height>

				<onleft>3</onleft>
				<onright>5</onright>
				<onclick>xbmc.playercontrol(play)</onclick>
				<onclick>SetFocus(4)</onclick>

				<texturefocus>osd/buttons/pause_focused.png</texturefocus>
				<texturenofocus>osd/buttons/pause_unfocused.png</texturenofocus>
			
				<alttexturefocus>osd/buttons/play_focused.png</alttexturefocus>
				<alttexturenofocus>osd/buttons/play_unfocused.png</alttexturenofocus>
				<usealttexture>Player.Paused</usealttexture>
			</control>
			<control type="button" id="5">
				<width>51</width>
				<height>51</height>
		
				<onleft>4</onleft>
				<onright>6</onright>

				<onclick>xbmc.playercontrol(forward)</onclick>

				<texturefocus>osd/buttons/fastfoward_focused.png</texturefocus>
				<texturenofocus>osd/buttons/fastfoward_unfocused.png</texturenofocus>
			</control>
			<control type="button" id="6">
				<width>51</width>
				<height>51</height>
		
				<onleft>5</onleft>
				<onright>7</onright>

				<onclick>xbmc.playercontrol(next)</onclick>

				<texturefocus>osd/buttons/next_focused.png</texturefocus>
				<texturenofocus>osd/buttons/next_unfocused.png</texturenofocus>
			</control>
		</control>

		<control type="progress">
			<posx>0</posx>
			<posy>637</posy>
			<width>1280</width>
			<height>25</height>
			<visible>true</visible>
			<reveal>true</reveal>
			<texturebg>osd/progress_bg.png</texturebg>
			<midtexture>osd/progress_center.png</midtexture>
			<lefttexture>osd/progress_left.png</lefttexture>
			<righttexture>osd/progress_right.png</righttexture>
			<overlaytexture>-</overlaytexture>
			<info>Player.Progress</info>
		</control>

		<control type="label">
			<width>300</width>
			<posx>-17</posx>
			<posy>674</posy>
		
			<font>Small</font>
			<textcolor>ffe8e8db</textcolor>
			<label>$INFO[Player.Time]/$INFO[Player.Duration]</label>
			<align>center</align>
		</control>

		<control type="image">
			<posx>0</posx>
			<posy>637</posy>
		
			<width>1280</width>
			<height>83</height>
		
			<texture>osd/osd_overlay.png</texture>
			<aspectratio>-</aspectratio>
		</control>


		<control type="group">
			<width>1280</width>
			<height>720</height>

			<visible>!Player.Playing</visible>
			<control type="label">
				<visible>Player.Paused</visible>
				<width>1280</width>
				<height>720</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>Paused</label>
				<font>OSD_Alert</font>
				<textcolor>c8e8e8db</textcolor>
			</control>		
			<control type="label">
				<visible>Player.Seeking</visible>
				<width>1280</width>
				<height>720</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>Seeking</label>
				<font>OSD_Alert</font>
				<textcolor>c8e8e8db</textcolor>
			</control>			
			<control type="label">
				<visible>Player.Forwarding2x</visible>
				<width>1280</width>
				<height>720</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>Fast Forward 2x</label>
				<font>OSD_Alert</font>
				<textcolor>c8e8e8db</textcolor>
			</control>
			<control type="label">
				<visible>Player.Forwarding4x</visible>
				<width>1280</width>
				<height>720</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>Fast Forward 4x</label>
				<font>OSD_Alert</font>
				<textcolor>c8e8e8db</textcolor>
			</control>
			<control type="label">
				<visible>Player.Forwarding8x</visible>
				<width>1280</width>
				<height>720</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>Fast Forward 8x</label>
				<font>OSD_Alert</font>
				<textcolor>c8e8e8db</textcolor>
			</control>
			<control type="label">
				<visible>Player.Forwarding16x</visible>
				<width>1280</width>
				<height>720</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>Fast Forward 16x</label>
				<font>OSD_Alert</font>
				<textcolor>c8e8e8db</textcolor>
			</control>
			<control type="label">
				<visible>Player.Forwarding32x</visible>
				<width>1280</width>
				<height>720</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>Fast Forward 32x</label>
				<font>OSD_Alert</font>
				<textcolor>c8e8e8db</textcolor>
			</control>
			<control type="label">
				<visible>Player.Rewinding2x</visible>
				<width>1280</width>
				<height>720</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>Rewind 2x</label>
				<font>OSD_Alert</font>
				<textcolor>c8e8e8db</textcolor>
			</control>
			<control type="label">
				<visible>Player.Rewinding4x</visible>
				<width>1280</width>
				<height>720</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>Rewind 4x</label>
				<font>OSD_Alert</font>
				<textcolor>c8e8e8db</textcolor>
			</control>
			<control type="label">
				<visible>Player.Rewinding8x</visible>
				<width>1280</width>
				<height>720</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>Rewind 8x</label>
				<font>OSD_Alert</font>
				<textcolor>c8e8e8db</textcolor>
			</control>
			<control type="label">
				<visible>Player.Rewinding16x</visible>
				<width>1280</width>
				<height>720</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>Rewind 16x</label>
				<font>OSD_Alert</font>
				<textcolor>c8e8e8db</textcolor>
			</control>
			<control type="label">
				<visible>Player.Rewinding32x</visible>
				<width>1280</width>
				<height>720</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>Rewind 32x</label>
				<font>OSD_Alert</font>
				<textcolor>c8e8e8db</textcolor>
			</control>
			<control type="label">
				<visible>Player.Caching + !Player.Playing</visible>
				<width>1280</width>
				<height>720</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>Caching...</label>
				<font>OSD_Alert</font>
				<textcolor>c8e8e8db</textcolor>
			</control>
		</control>




